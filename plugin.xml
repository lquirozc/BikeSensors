<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-bikesensors" version="1.0.0"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>BikeSensors</name>

    <js-module name="BikeSensors" src="www/BikeSensors.js">
        <clobbers target="BikeSensors" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="BikeSensors">
                <param name="android-package" value="cordova.plugin.bikesensors.BikeSensors" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/BikeSensors.java" target-dir="src/cordova/plugin/bikesensors/BikeSensors" />
        <source-file src="src/android/MachineInfo.java" target-dir="src/cordova/plugin/bikesensors/BikeSensors" />
    </platform>
</plugin>